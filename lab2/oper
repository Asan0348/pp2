x = 5
x += 5
print(x)
x = 5
x -= 5
print(x)
x = 5
x /= 5
print(x)
x = 5
x *= 2
print(x)
x = 5
x %= 3
print(x)
x = 5
x //= 3
print(x)
x = 5
x **= 2
print(x)
x = 5
x &= 3
print(f"{x} - is &")
x = 5
x |= 4
print(f"{x} - is |")
x = 5
x ^= 4
print(f"{x} - is xor")
x = 5
x >>= 7
print(f"{x} - is >>")
x = 5
x <<= 7
print(f"{x} - is <<")
x = 5
print(x := 8)
