set1 = {"a", "b", "c"}
set2 = {1, 2, 3}

set3 = set1.union(set2)
print(set3)

set1 = {"apple", "banana", "cherry"}
set2 = {"google", "microsoft", "apple"}

set3 = set1.intersection(set2) #дубликаты
print(set3)

set3 = set1.difference(set2) #то что есть только в 1

print(set3)


set3 = set1.symmetric_difference(set2) #сохр только то что нет в обоих

print(set3)
