thislist = ["apple", "banana", "cherry"]
tropical = ["mango", "pineapple", "papaya"]
thislist.extend(tropical)
print(thislist)
print("------")

thislist.remove("banana")
print(thislist)
print("------")

thislist.pop(1)
print(thislist)
print("-------")

del thislist[0]
print(thislist)

thislist.clear()
print(thislist)

del thislist
